            if ($('#hdnConfigureType').val() == "1") {
                $.post("@Url.Content("~/Frontal/Vendedor/WriteScriptSettings")", { CustomModuleAttributeList: mainElementObject, ChildModuleAttributeList: childElementObject }, function (data) {
                    $('#hdnConfigureType').val("0");
